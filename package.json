{
  "name": "@ignitial/iio-app-template",
  "version": "5.0.0",
  "description": "Ignitial.io Vue application template v5",
  "author": "Victor Andritoiu <andritoiu@ignitial.fr>",
  "repository": "git@github.com:ignitialio/iio-app-template.git",
  "main": "test/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "server": "node ./server/index.js",
    "start:dev": "npm-run-all --parallel server serve",
    "start:prod": "npm run build && npm run server",
    "lint": "eslint --ext .js,.vue src",
    "build:image": "docker build --rm -t ignitial/iioat:${npm_package_version} . && docker tag ignitial/iioat:${npm_package_version} registry.ignitial.io/ignitial/iioat:${npm_package_version}",
    "build:dlake:image": "docker build --rm -f Dockerfile.dlake -t ignitial/dlake-iioat:${npm_package_version} . && docker tag ignitial/dlake-iioat:${npm_package_version} registry.ignitial.io/ignitial/dlake-iioat:${npm_package_version}",
    "publish:docker": "npm run build:image && docker push registry.ignitial.io/ignitial/iioat:${npm_package_version}",
    "publish:dlake:docker": "npm run build:dlake:image && docker push registry.ignitial.io/ignitial/dlake-iioat:${npm_package_version}",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*"
  },
  "dependencies": {
    "@ignitial/iio-app-server": "^4.1.3",
    "babel-polyfill": "^6.26.0",
    "d3": "^5.9.2",
    "material-design-icons": "^3.0.1",
    "register-service-worker": "^1.6.2",
    "typeface-roboto": "0.0.54",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-service": "^3.8.3",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mongodb": "^3.2.5",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^1.0.4",
    "vue-template-compiler": "^2.6.10",
    "webpack-dev-server": "^3.5.1"
  },
  "license": "MIT",
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
